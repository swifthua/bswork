<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.zh.po.Depart" table="DEPART" >
        <id name="bmdm" type="java.lang.String">
            <column name="BMDM" length="16" />
            <generator class="assigned" /><!-- assigned手动分配方式 -->
        </id>
        <property name="bmmc" type="java.lang.String">
            <column name="BMMC" length="32" />
        </property>
        <property name="lastUpdate" type="java.util.Date">
            <column name="LASTUPDATE"  />
        </property>
        <property name="sfjy" type="java.lang.Integer">
            <column name="SFJY"  />
        </property>
    </class>
</hibernate-mapping>
