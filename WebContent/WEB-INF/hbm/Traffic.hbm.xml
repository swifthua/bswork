<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.zh.po.Traffic" table="TRAFFIC" >
        <id name="gjid" type="java.lang.String">
            <column name="GJID" length="16" />
            <generator class="assigned" /><!-- assigned手动分配方式 -->
        </id>
        <property name="gjms" type="java.lang.String">
            <column name="GJMS" length="16" />
        </property>
        <property name="sfjy" type="java.lang.Integer">
            <column name="SFJY"  />
        </property>
    </class>
</hibernate-mapping>
