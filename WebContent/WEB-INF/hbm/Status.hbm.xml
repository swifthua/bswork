<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.zh.po.Status" table="STATUS" >
        <id name="ztid" type="java.lang.String">
            <column name="ZTID" length="16"/>
            <generator class="assigned" />
        </id>
        <property name="ztms" type="java.lang.String">
            <column name="ZTMS" length="16" />
        </property>
        <property name="sfjy" type="java.lang.Integer">
            <column name="SFJY" />
        </property>
    </class>
</hibernate-mapping>
